         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"bits", "=", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"type", "===", "\"\<Constant\>\""}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"(*", " ", 
           RowBox[{
           "All", " ", "0", "s", " ", "or", " ", "All", " ", "1", "s"}], " ", 
           "*)"}], "\[IndentingNewLine]", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"RandomInteger", "[", "]"}], "==", "0"}], ",", 
            "\[IndentingNewLine]", 
            RowBox[{"ConstantArray", "[", 
             RowBox[{"0", ",", 
              RowBox[{"2", "^", "n"}]}], "]"}], ",", 
            RowBox[{"ConstantArray", "[", 
             RowBox[{"1", ",", 
              RowBox[{"2", "^", "n"}]}], "]"}]}], "]"}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"(*", " ", 
           RowBox[{
