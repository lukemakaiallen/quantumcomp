     "Quantum", " ", "and", " ", "Classical"}]}], "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"makeTruthTable", "[", "n_Integer", "]"}], " ", ":=", " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"type", ",", " ", "bits"}], "}"}], ",", " ", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"type", " ", "=", " ", 
       RowBox[{"RandomChoice", "[", 
        RowBox[{"{", 
         RowBox[{"\"\<Constant\>\"", ",", " ", "\"\<Balanced\>\""}], "}"}], 
        "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"bits", "=", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"type", "===", "\"\<Constant\>\""}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"(*", 
          RowBox[{
          "All", " ", "0", "s", " ", "or", " ", "All", " ", "1", "s"}], 
          "*)"}], "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"RandomInteger", "[", "]"}], "==", "0"}], ",", 
           RowBox[{"ConstantArray", "[", 
            RowBox[{"0", ",", 
             RowBox[{"2", "^", "n"}]}], "]"}], ",", 
           RowBox[{"ConstantArray", "[", 
            RowBox[{"1", ",", 
             RowBox[{"2", "^", "n"}]}], "]"}]}], "]"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"(*", 
          RowBox[{"Balanced", ":", 
           RowBox[{
           "Shuffle", " ", "equal", " ", "number", " ", "of", " ", "0", "s", " ",
             "and", " ", "1", "s"}]}], "*)"}], "\[IndentingNewLine]", 
         RowBox[{"RandomSample", "[", 
          RowBox[{"Join", "[", 
