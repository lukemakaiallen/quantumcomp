            RowBox[{"ConstantArray", "[", 
             RowBox[{"0", ",", 
              RowBox[{"2", "^", 
               RowBox[{"(", 
                RowBox[{"n", "-", "1"}], ")"}]}]}], "]"}], ",", 
            RowBox[{"ConstantArray", "[", 
             RowBox[{"1", ",", 
              RowBox[{"2", "^", 
               RowBox[{"(", 
                RowBox[{"n", "-", "1"}], ")"}]}]}], "]"}]}], "]"}], "]"}]}], 
        "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
       "return", " ", "bits", " ", "and", " ", "hidden", " ", "type", " ", 
        "for", " ", "verification"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"<|", 
       RowBox[{
        RowBox[{"\"\<Bits\>\"", "->", " ", "bits"}], ",", " ", 
        RowBox[{"\"\<Type\>\"", "->", " ", "type"}]}], "|>"}]}]}], 
