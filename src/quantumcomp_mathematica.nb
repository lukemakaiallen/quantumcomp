Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{
    "use", " ", "TruthTable", " ", "generator", " ", "to", " ", "create", " ",
      "random", " ", "function", " ", "f", 
